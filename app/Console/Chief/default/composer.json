{
  "name": "thinktomorrow/chief",
  "description": "Chief skeleton application",
  "keywords": ["framework", "laravel", "thinktomorrow"],
  "license": "none",
  "type": "project",
  "require": {
    "php": ">=7.0",
    "bugsnag/bugsnag-laravel": "^2.4",
    "consoletvs/charts": "5.*",
    "dimsav/laravel-translatable": "^8.0",
    "doctrine/dbal": "^2.5",
    "laravel/framework": "5.5.*",
    "laravel/tinker": "^1.0",
    "spatie/laravel-image-optimizer": "^1.0",
    "spatie/laravel-permission": "^2.1",
    "thinktomorrow/locale": "^1.3",
    "thinktomorrow/squanto": "^0.3",
    "thinktomorrow/assetlibrary": "^0.3"
  },
  "require-dev": {
    "barryvdh/laravel-debugbar": "^3.1",
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "1.0.*",
    "phpunit/phpunit": "~6.5"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Chief\\" : "chief-core/"
    },
    "files": [
      "chief-core/utils/vendors/htmlLawed.php",
      "chief-core/utils/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
