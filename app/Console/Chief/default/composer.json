{
  "name": "thinktomorrow/##PROJECT_NAME##",
  "description": "##PROJECT_NAME## site development",
  "keywords": ["thinktomorrow", "laravel"],
  "license": "none",
  "type": "project",
  "require": {
    "php": ">=5.5.9",
    "laravel/framework": "5.2.*",
    "thinktomorrow/locale":"^0.0.6",
    "laravelcollective/html": "^5.2",
    "dimsav/laravel-translatable":"^5.4",
    "bugsnag/bugsnag-laravel": "^1.6"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~4.0",
    "symfony/css-selector": "2.8.*|3.0.*",
    "symfony/dom-crawler": "2.8.*|3.0.*"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Chief\\" : "chief-core/",
      "##PROJECT_NAME##\\" : "src/"
    },
    "files": [
      "chief-core/utils/vendors/htmlLawed.php",
      "chief-core/utils/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist"
  }
}
